{
  "name": "typs",
  "version": "0.3.7",
  "description": "An handy library for type validation in Javascript",
  "main": "typs-transpiled.js",
  "scripts": {
    "test": "babel-node test.js",
    "watch": "onchange typs.js test.js -- npm test",
    "build": "babel typs.js > typs-transpiled.js",
	"deploy": "git tag \"v%npm_package_version%\" && npm run build && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattecapu/typs.git"
  },
  "keywords": [
    "types",
    "type-checking",
    "validation",
    "tool"
  ],
  "author": "Matteo Capucci <mattecapu@live.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattecapu/typs/issues"
  },
  "homepage": "https://github.com/mattecapu/typs",
  "devDependencies": {
    "babel": "latest",
    "onchange": "^0.2.0"
  },
  "dependencies": {
    "bluebird": "^2.9.11"
  }
}
